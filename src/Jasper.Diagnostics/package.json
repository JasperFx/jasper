{
  "name": "jasper-diagnostics",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Joe McBride",
  "license": "MIT",
  "scripts": {
    "first": "yarn && yarn restore",
    "start": "better-npm-run run",
    "compile": "better-npm-run compile",
    "build": "better-npm-run build",
    "build:prod": "better-npm-run build:prod",
    "dotnetServer": "better-npm-run dotnetServer",
    "webpackServer": "better-npm-run webpackServer"
  },
  "betterScripts": {
    "run": {
      "command": "babel-node ./tools/build.js run",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dotnetServer": {
      "command": "babel-node ./tools/dotnetServer.js",
      "env": {
        "NODE_ENV": "development",
        "TARGET_APP": "../DiagnosticsHarness",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://localhost:5000"
      }
    },
    "webpackServer": {
      "command": "babel-node ./tools/webpackDevServer.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "compile": {
      "command": "babel-node ./tools/build.js compile",
      "env": {
        "NODE_ENV": "development",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CONFIGURATION": "Debug"
      }
    },
    "build": {
      "command": "babel-node ./tools/build.js build",
      "env": {
        "NODE_ENV": "development",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CONFIGURATION": "Debug"
      }
    },
    "build:prod": {
      "command": "babel-node ./tools/build.js build",
      "env": {
        "NODE_ENV": "production",
        "ASPNETCORE_ENVIRONMENT": "Production",
        "CONFIGURATION": "Release"
      }
    }
  },
  "dependencies": {
    "axios": "^0.15.3",
    "classnames": "^2.2.5",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-form": "^0.9.3",
    "react-redux": "^4.4.6",
    "react-router": "v4.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^1.0.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@webpack-blocks/babel6": "^0.1.1",
    "@webpack-blocks/dev-server": "^0.1.0",
    "@webpack-blocks/webpack": "^0.1.5",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "better-npm-run": "^0.0.14",
    "change-case": "^3.0.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "open": "^0.0.5",
    "react-hot-loader": "^1.3.1",
    "shelljs": "^0.7.5",
    "simple-make": "^1.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
