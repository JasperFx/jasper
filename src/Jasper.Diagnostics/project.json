{
  "version": "0.1.0-alpha-*",

  "buildOptions": {
    "debugType": "portable",
    "embed": {
      "include": [
        "resources/**/*.js",
        "resources/**/*.css",
        "resources/**/*.json"
      ]
    },
    "compile": [
      "**/*.cs",
      "../CommonAssemblyInfo.cs"
    ]
  },

  "description": "Diagnostics for Jasper Service Bus",
  "authors": [
      "Joe McBride",
      "Jeremy D. Miller"
  ],


  "packOptions": {
      "projectUrl": "http://jasperfx.github.io",
      "licenseUrl": "https://github.com/JasperFX/jasper/blob/master/LICENSE.txt",
      "iconUrl": "https://avatars2.githubusercontent.com/u/10048186?v=3&s=200"
  },

  "dependencies": {
    "Jasper": { "target": "project" },
    "JasperBus": { "target": "project" },
    "Newtonsoft.Json": "9.0.1",
    "StructureMap": "4.4.3",
    "StructureMap.Microsoft.DependencyInjection": "1.3.0",
    "Microsoft.AspNetCore.Hosting": "1.0.0",
    "Microsoft.AspNetCore.Http.Abstractions": "1.1.0",
    "Microsoft.AspNetCore.Http.Extensions": "1.1.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Microsoft.AspNetCore.WebSockets": "1.0.0"
  },
  "frameworks": {
    "netstandard1.5": {
      "dependencies": {
        "NETStandard.Library": "1.6.1"
      }
    }
  }
}
